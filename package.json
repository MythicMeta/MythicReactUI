{
  "name": "mythic",
  "version": "0.1.0",
  "private": true,
  "homepage": "/new",
  "author": "@its_a_feature_",
  "description": "React UI for Mythic C2 Framework",
  "build": {
    "appId": "com.itsafeature.mythic",
    "compression": "normal",
    "files": ["build/**"],
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "mac": {
      "icon": "build/mythic.svg",
      "type": "distribution",
      "target": ["pkg", "dmg"]
    },
    "win": {
      "target": "exe"
    },
    "linux": {
      "icon": "public/mythic.svg",
      "target": ["snap", "AppImage"],
      "category": "C2"
    }
  },
  "main": "public/electron.js",
  "devDependencies": {
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "react-scripts": "^5.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@date-io/date-fns": "^1.3.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "@welldone-software/why-did-you-render": "^6.1.0",
    "ace-builds": "^1.4.12",
    "arrgv": "^1.0.2",
    "babel-plugin-macros": "^2.8.0",
    "d3": "^5.16.0",
    "dagre-d3": "^0.6.4",
    "date-fns": "^2.22.1",
    "electron-is-dev": "^2.0.0",
    "graphql": "^15.4.0",
    "graphql-ws": "^3.2.0",
    "jwt-decode": "^3.1.2",
    "notistack": "^1.0.3",
    "path-browserify": "^1.0.1",
    "randexp": "^0.5.3",
    "react": "^17.0.1",
    "react-ace": "^9.4.1",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.4",
    "react-router-dom": "^5.2.0",
    "react-scrollbar-size": "^4.0.0",
    "react-virtualized": "^9.22.3",
    "react-virtualized-auto-sizer": "^1.0.5",
    "react-vtree": "^3.0.0-beta.1",
    "react-window": "^1.8.6",
    "save-svg-as-png": "^1.4.17",
    "styled-components": "^5.2.1",
    "subscriptions-transport-ws": "^0.9.18",
    "util": "^0.12.4",
    "wait-on": "^6.0.0",
    "ws": "^7.4.1",
    "yargs-parser": "^18.1.3"
  },
  "scripts": {
    "react-start": "BROWSER=none react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder build --publish never",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
